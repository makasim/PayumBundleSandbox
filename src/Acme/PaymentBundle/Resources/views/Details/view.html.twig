{% extends "AcmePaymentBundle::layout.html.twig" %}

{% block title %}
    {{ paymentTitle }} - Details View"
{% endblock %}

{% block content %}
    <div class="block">
        {% include "AcmePaymentBundle:Details:_status.html.twig" with { 'status': status } only %}
    </div>

    <div class="block">
        <ul>
            {% for key, value in status.model %}
                <li>{{ key }}: {{ value|json_encode|raw }}</li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}

{% set code = code(_self) %}